{
	"compilerOptions": {
		"module": "none",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"outFile": "build/spine-all.js",
		"sourceMap": true,
		"declaration": true,
		"lib": [
			"DOM",
			"ES2015",
			"WebWorker.ImportScripts"
		]
	},
	"include": [
		"core/src/**/*",
		"canvas/src/**/*",
		"webgl/src/**/*",
		"threejs/src/**/*",
		"player/src/**/*"
	],
	"exclude": [
		"build"
	]
}